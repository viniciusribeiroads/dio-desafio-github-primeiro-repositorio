package entities;

public class Conta {
	double saldo;
	int agencia;
	int numero;
	String titular;
	
	public Conta(){
		
	}

	public Conta(double saldo, int agencia, int numero, String titular) {
		super();
		this.saldo = saldo;
		this.agencia = agencia;
		this.numero = numero;
		this.titular = titular;
	}
	
	
}
